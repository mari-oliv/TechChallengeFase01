{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb",
        "includeFiles": [
          "views//*",
          "routers//*",
          "data_extraction//*"
        ],
        "excludeFiles": [
          "env//*",
          "pycache//*",
          "assets//*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/routers/vitibrasil.py"
    }
  ]
}